{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://$BACKEND_URL/api/$1"
    },
    {
      "src": "^(?!/api)(.*)$",
      "dest": "/$1"
    }
  ]
}
